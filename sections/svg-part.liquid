<style>

  .svg-section {
  display: flex;
  justify-content: center;
}

.svg-section img {
  flex: 0 0 auto;
  max-width: 100%;
  height: auto;
  margin: 10px;
}

@media (max-width: 768px) {
  .svg-section img {
    flex: 0 0 50%;
  }
}

@media (max-width: 480px) {
  .svg-section img {
    flex: 0 0 100%;
  }
}

</style>



{% schema %}
  {
    "name": "SVG Section",
    "class": "svg-section",
    "settings": [],
    "blocks": [],
    "max_blocks": 1,
    "presets": [
      {
        "category": "svg",
        "name": "svg"
      }
    ]
  }
{% endschema %}

